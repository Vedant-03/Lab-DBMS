<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]>      <html class="no-js"> <!--<![endif]-->
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge">
        <title>Chemistry Laboratory</title>
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width, initial-scale=1">
        <link rel="stylesheet" href="/CSS/lab.css">
    </head>
    <body>
        <hr size="2" noshade>
        <h1 style="text-align: center;"><a href="/">Laboratory Database Management System</a></h1>
        <hr size="2" noshade>
        <div>
            <h4>Room: 1</h4>
            <table>
                <thead>
                    <tr>
                        <th>Item ID</th>
                        <th>Item</th>
                        <th>Model</th>
                        <th>Quantity</th>
                        <th>About</th>
                    </tr>
                </thead>
                <tbody>
                    <% if(data[0].length){
                        for(var i = 0; i < data[0].length; i++){ 
                            if(data[0][i].i_id){%>
                                <tr>
                                    <td><%= data[0][i].i_id %></td>
                                    <td><img src="<%= data[0][i].image %>" alt=""></td>
                                    <td><%= data[0][i].model %></td>
                                    <td><%= data[0][i].quantity %></td>
                                    <td><%= data[0][i].descr %></td>
                                </tr>
                            <% }
                        }
                    }
                    else{ %>
                        <tr>
                            <td>No items to display</td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>

        <div>
            <h4>Room: 2</h4>
            <table>
                <thead>
                    <tr>
                        <th>Item ID</th>
                        <th>Item</th>
                        <th>Model</th>
                        <th>Quantity</th>
                        <th>About</th>
                    </tr>
                </thead>
                <tbody>
                    <% if(data[1].length){
                        for(var i = 0; i < data[1].length; i++){ 
                            if(data[1][i].i_id){%>
                                <tr>
                                    <td><%= data[1][i].i_id %></td>
                                    <td><img src="<%= data[1][i].image %>" alt=""></td>
                                    <td><%= data[1][i].model %></td>
                                    <td><%= data[1][i].quantity %></td>
                                    <td><%= data[1][i].descr %></td>
                                </tr>
                            <% }
                        }
                    }
                    else{ %>
                        <tr>
                            <td>No items to display</td>
                        </tr>
                    <% } %>
                </tbody>
            </table>
        </div>

        <script src="" async defer></script>
    </body>
</html>